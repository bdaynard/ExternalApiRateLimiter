{
  "ConnectionStrings": {
    "Cache": "redis:6379"
  },

  "RateLimitSettings": {
    "NumberLimits": {
      "PermitLimit": 100,
      "WindowInSeconds": 1
    },
    "GlobalLimits": {
      "PermitLimit": 100000,
      "WindowInSeconds": 1
    }
  },

  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Debug"
    }
  },
  "AllowedHosts": "*"
}
